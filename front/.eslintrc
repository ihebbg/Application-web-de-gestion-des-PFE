{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },

        "ecmaVersion": 7,
        "sourceType": "module",
        "allowImportExportEverywhere": true
      
    },
     "extends": [
         "eslint:recommended",    
        "plugin:react/recommended","eslint-config-prettier"
     ],
    "rules": {
        "react/prop-types": "off",
        "react/no-unescaped-entities": 0,
        "quotes":["error","single"],
        "semi": "off"
    },
    "settings": {
        "react": {
          "version": "detect"
        }},
    
        "env": {
            "browser":true  
        }
}